{
    "name": "passchn/cakephp-logging",
    "description": "Logging plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "cakephp/cakephp": "^4.4 || ^5.0",
        "psr/log": "^3.0",
        "passchn/cakephp-simple-di": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Passchn\\CakeLogging\\": "src/",
            "Passchn\\CakeLogging\\Tests\\Stub\\": "tests/Stub/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/test_app/src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
